{
  "name": "ac-countrylist",
  "version": "1.0.1",
  "dependencies": {
    "lodash": "^4.17.15"
  },
  "repository": "mmpro/ac-countrylist",
  "license": "MIT",
  "devDependencies": {
    "ac-conventional-changelog-template": "git+https://github.com/mmpro/ac-conventional-changelog-template.git",
    "corp-semantic-release": "git+https://github.com/mmpro/corp-semantic-release.git",
    "cz-customizable": "git+https://github.com/mmpro/cz-customizable.git",
    "eslint": "^6.0.1",
    "expect": "^24.7.1",
    "husky": "^3.0.2",
    "mocha": "^6.1.4",
    "mocha-jenkins-reporter": "^0.4.2",
    "mocha-junit-reporter": "^1.23.1"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".czConfig.js"
    }
  },
  "scripts": {
    "test": "mocha --bail --colors --reporter spec",
    "test-jenkins": "JUNIT_REPORT_PATH=./report.xml mocha --bail --colors --reporter mocha-jenkins-reporter --reporter-options junit_report_name='AC-CountryList'"
  }
}
